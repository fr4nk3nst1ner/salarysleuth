# SalarySleuth Job Tracker Environment Variables
# Copy this file to .env and fill in your credentials

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Scraper Settings
SCRAPE_PAGES=20
SCRAPE_DESCRIPTION=Offensive Security

# Timezone
TZ=America/Chicago

# Web Authentication (Optional - leave empty to disable)
# Set these to enable HTTP Basic Authentication for the web interface
WEB_USERNAME=admin
WEB_PASSWORD=your-secure-password-here
